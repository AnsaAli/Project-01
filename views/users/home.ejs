                <script>

                    document.addEventListener('DOMContentLoaded', function () {
                        const quantitySelects = document.querySelectorAll('.quantity-select');

                        quantitySelects.forEach(select => {
                            const productId = select.id.split('-').pop();
                            const pricesContainer = document.getElementById(`prices-container-${productId}`);
                            const offerPricesContainer = document.getElementById(`offer-prices-container-${productId}`);

                            select.addEventListener('change', function (event) {
                                const selectedQuantity = this.value;
                                const prices = pricesContainer.querySelectorAll('.price');
                                const offerPrices = offerPricesContainer.querySelectorAll('.offer-price');

                                prices.forEach(price => {
                                    price.style.display = price.dataset.quantity === selectedQuantity ? 'block' : 'none';
                                });

                                offerPrices.forEach(offerPrice => {
                                    offerPrice.style.display = offerPrice.dataset.quantity === selectedQuantity ? 'block' : 'none';
                                });
                            });
                        });
                    });






                </script>


